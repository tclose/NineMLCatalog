<?xml version="1.0" encoding="UTF-8"?>
<NineML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://nineml.net/9ML/1.0"
  xsi:schemaLocation="http://nineml.incf.org/9ML/NineML_v0.3.xsd">
  <Dimension name="per_time" t="-1"/>
  <Dimension name="voltage" m="1" l="2" t="-3" i="-1"/>  
  <Dimension name="temperature" k="1"/>
  <Dimension name="length" l="1"/>
  <Dimension name="conductanceDensity" m="-1" l="-4" t="3" i="2"/>
  <Dimension name="currentDensity" i="1" l="-2"/>
  <Dimension name="concentration" l="-3" n="1"/>
  <Dimension name="dimensionless"/>
  <Dimension name="idealGasConstantDims" m="1" l="2" t="-2" n="-1" k="-1"/>
  <Dimension name="charge_per_mole" t="1" i="1" n="-1"/>
  <Unit symbol="C_per_mol" dimension="charge_per_mole" power="0"/>
  <Unit symbol="J_per_K_mol" dimension="idealGasConstantDims" power="0"/>  
  <Unit symbol="celsius" dimension="temperature" power="0" offset="273.15"/>
  <Unit symbol="unitless" dimension="dimensionless" power="0"/>
  <Unit symbol="K" dimension="temperature" power="0"/>
  <Dynamics name="CalciumPoolGHK">
    <Parameter name="diameter" dimension="length"/>
    <Parameter name="steadyStateInternalC" dimension="concentration"/>
    <Parameter name="externalC" dimension="concentration"/>
    <Parameter name="beta" dimension="per_time"/>
    <Parameter name="e_reversal" dimension="voltage"/>
    <AnalogReceivePort name="g" dimension="conductanceDensity"/>
    <AnalogReceivePort name="temperature" dimension="temperature"/>
    <AnalogReceivePort name="v" dimension="voltage"/>
    <AnalogSendPort name="i" dimension="currentDensity"/>
    <AnalogSendPort name="internalC" dimension="concentration"/>
    <StateVariable name="internalC" dimension="concentration">
      <Initial>steadyStateInternalC</Initial>
    </StateVariable>
    <Regime name="default">
      <TimeDerivative variable="internalC">
        <MathInline>
          -i / (2 * Faraday * diameter) - 
          beta * (internalC - steadyStateInternalC)
        </MathInline>
      </TimeDerivative>
    </Regime>
    <Alias name="i">
      <MathInline>g * h * (v - e_reversal)</MathInline>
    </Alias>
    <Alias name="h">
      <MathInline>denominator &lt;= smallDenominator ? smallDenominatorCutOff : nonSmallDenominator</MathInline>
    </Alias>
    <Alias name="nonSmallDenominator">
      <MathInline>CaValence * Faraday * (internalC - externalC * exp(-zeta)) * (1 + zeta / 2)</MathInline>
    </Alias>
    <Alias name="smallDenominator">
      <MathInline>CaValence * Faraday * zeta * (internalC - externalC * exp(-zeta)) / denominator</MathInline>
    </Alias>
    <Alias name="zeta">
      <MathInline>
        (Faraday * CaValence * v) / 
        (UniversalGasConstant * (temperature - AbsoluteZero))
      </MathInline>
    </Alias>
    <Alias name="denominator">
      <MathInline>1 - exp(-zeta)</MathInline>
    </Alias>
    <Constant name="smallDenominatorCutOff" value="1e-6" units="unitless"/>
    <Constant name="Faraday" value="96485.309" units="C_per_mol"/>
    <Constant name="UniversalGasConstant" value="8.314472" units="J_per_K_mol"/>
    <Constant name="AbsoluteZero" value="0" units="K"/>
    <Constant name="CaValence" value="2" units="unitless"/>
  </Dynamics>
</NineML>
