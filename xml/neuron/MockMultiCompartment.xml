<?xml version="1.0" encoding="UTF-8"?>
<NineML xmlns="http://nineml.net/9ML/2.0">
  <MultiCompartment name="ExampleMultiCompartmentModel">
    <Tree>
      <ExternalArrayValue url="./example_compartments.txt" mimetype="vnd.net.nineml.arrayvalue.text"
        columnName="parentID"/>
    </Tree>
    <Mapping>
      <ExternalArrayValue url="./example_compartments.txt" mimetype="vnd.net.nineml.arrayvalue.text"
        columnName="domain"/>
    </Mapping>
    <Domain index="0">
      <MultiDynamics name="somaDynamics">
        <SubDynamics name="PyramidalNa">
          <Dynamics>
            <Prototype name="HHPyramidalNa" url="http://nineml.net/9ML/2.0/catalog/channels"/>
            <Property name="gbar">
              <Quantity units="uS">
                <SingleValue>0.0036</SingleValue>
              </Quantity>
            </Property>
          </Dynamics>
          <PortConnection port="V">
            <FromSister component="membrane" port="V"/>
          </PortConnection>
        </SubDynamics>
        <SubDynamics name="PyramidalK">
          <Dynamics>
            <Prototype name="HHPyramidalK" url="http://nineml.net/9ML/2.0/catalog/channels"/>
            <Property name="gbar">
              <Quantity units="uS">
                <SingleValue>0.0025</SingleValue>
              </Quantity>
            </Property>
          </Dynamics>
          <PortConnection port="V">
            <FromSister component="membrane" port="V"/>
          </PortConnection>
        </SubDynamics>
        <SubDynamics name="Kv4">
          <Dynamics>
            <Prototype name="HHPyramidalK" url="http://nineml.net/9ML/2.0/catalog/channels"/>
            <Property name="gbar">
              <Quantity units="uS">
                <SingleValue>0.0025</SingleValue>
              </Quantity>
            </Property>
          </Dynamics>
          <PortConnection port="V">
            <FromSister component="membrane" port="V"/>
          </PortConnection>
        </SubDynamics>
        <SubDynamics name="leak">
          <Dynamics>
            <Definition name="Leak" url="http://nineml.net/9ML/2.0/catalog/channels"/>
            <Property name="g">
              <Quantity units="uS">
                <SingleValue>0.001</SingleValue>
              </Quantity>
            </Property>
            <Property name="e_rev">
              <Quantity units="mV">
                <SingleValue>-21</SingleValue>
              </Quantity>
            </Property>
          </Dynamics>
          <PortConnection port="V">
            <FromSister component="membrane" port="V"/>
          </PortConnection>
        </SubDynamics>
        <SubDynamics name="membrane">
          <Dynamics>
            <Definition name="BasicCompartment" url="http://nineml.net/9ML/2.0/catalog/channels/basicCompartment.9ml"/>
            <Property name="length">
              <Quantity units="um">
                <SingleValue>15.0</SingleValue>
              </Quantity>
            </Property>
            <Property name="axialResistance">
              <Quantity units="ohm_per_cm">
                <SingleValue>100.0</SingleValue>
              </Quantity>
            </Property>
            <Property name="diameter">
              <Quantity units="um">
                <SingleValue>15.0</SingleValue>
              </Quantity>
            </Property>
            <Property name="C">
              <Quantity units="uF">
                <SingleValue>1.0</SingleValue>
              </Quantity>
            </Property>
          </Dynamics>
          <PortConnection port="i">
            <FromSister component="Na" name="i"/>
            <FromSister component="K" name="i"/>
            <FromSister component="leak" name="i"/>
          </PortConnection>
        </SubDynamics>
        <PortExposure name="proximalVoltage" component="membrane" port="proximalV"/>
        <PortExposure name="distalVoltage" component="membrane" port="distalV"/>
      </MultiDynamics>
      <PortConnection port="proximalVoltage">
        <FromProximal port="voltage"/>
      </PortConnection>
      <PortConnection port="distalVoltage">
        <FromDistal port="voltage"/>
      </PortConnection>
    </Domain>
    <Domain index="1">
      <Reference>DendriteDynamics</Reference>
      <PortConnection port="proximalVoltage">
        <FromProximal port="voltage"/>
      </PortConnection>
      <PortConnection port="distalVoltage">
        <FromDistal port="voltage"/>
      </PortConnection>
      <PortConnection port="withinDendritesReducePort">
        <FromProximal domain="dendrites" port="withinDendritesSendPort"/>
        <FromDistal domain="dendrites" port="withinDendritesSendPort"/>
      </PortConnection>
    </Domain>
  </MultiCompartment>
  <Dimension name="time" t="1"/>
  <Dimension name="voltage" m="1" l="2" t="-3" i="-1"/>
  <Dimension name="conductance" m="-1" t="3" l="-2" i="2"/>
  <Dimension name="area" l="2"/>
  <Dimension name="length" l="1"/>
  <Unit symbol="mV" dimension="voltage" power="-3"/>
  <Unit symbol="um" dimension="length" power="-3"/>
  <Unit symbol="ms" dimension="time" power="-3"/>
  <Unit symbol="cm_square" dimension="area" power="-4"/>
  <Unit symbol="mS" dimension="conductance" power="-3"/>
</NineML>
