<?xml version="1.0" encoding="UTF-8"?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <Dynamics name="HodgkinHuxley">
    <Parameter name="m_alpha_A" dimension="per_time_voltage"/>
    <Parameter name="m_alpha_V0" dimension="voltage"/>
    <Parameter name="m_alpha_K" dimension="voltage"/>
    <Parameter name="m_beta_A" dimension="per_time_voltage"/>
    <Parameter name="m_beta_V0" dimension="voltage"/>
    <Parameter name="m_beta_K" dimension="voltage"/>
    <Parameter name="h_alpha_A" dimension="per_time"/>
    <Parameter name="h_alpha_V0" dimension="voltage"/>
    <Parameter name="h_alpha_K" dimension="voltage"/>
    <Parameter name="h_beta_A" dimension="per_time"/>
    <Parameter name="h_beta_V0" dimension="voltage"/>
    <Parameter name="h_beta_K" dimension="voltage"/>
    <Parameter name="n_alpha_A" dimension="per_time_voltage"/>
    <Parameter name="n_alpha_V0" dimension="voltage"/>
    <Parameter name="n_alpha_K" dimension="voltage"/>
    <Parameter name="n_beta_A" dimension="per_time"/>
    <Parameter name="n_beta_V0" dimension="voltage"/>
    <Parameter name="n_beta_K" dimension="voltage"/>
    <Parameter name="gbarNa" dimension="conductance"/>
    <Parameter name="gbarK" dimension="conductance"/>
    <Parameter name="gLeak" dimension="conductance"/>
    <Parameter name="eLeak" dimension="voltage"/>
    <Parameter name="eK" dimension="voltage"/>
    <Parameter name="eNa" dimension="voltage"/>
    <Parameter name="C" dimension="capacitance"/>
    <Parameter name="v_rest" dimension="voltage"/>
    <Parameter name="v_threshold" dimension="voltage"/>
    <AnalogReducePort name="iExt" dimension="current" operator="+"/>
    <EventSendPort name="outgoingSpike"/>
    <StateVariable name="v" dimension="voltage"/>
    <StateVariable name="m" dimension="dimensionless"/>
    <StateVariable name="h" dimension="dimensionless"/>
    <StateVariable name="n" dimension="dimensionless"/>
    <Regime name="default">
      <TimeDerivative variable="v">
        <MathInline>(iExt - iNa - iK - iLeak) / C</MathInline>
      </TimeDerivative>
      <TimeDerivative variable="m">
        <MathInline>(m_inf - m) / m_tau</MathInline>
      </TimeDerivative>
      <TimeDerivative variable="h">
        <MathInline>(h_inf - h) / h_tau</MathInline>
      </TimeDerivative>
      <TimeDerivative variable="n">
        <MathInline>(n_inf - n) / n_tau</MathInline>
      </TimeDerivative>
      <OnCondition target_regime="default">
        <Trigger>
          <MathInline>v &gt; v_threshold</MathInline>
        </Trigger>
        <OutputEvent port="outgoingSpike"/>
      </OnCondition>
    </Regime>
    <Alias name="iK">
      <MathInline>gbarK * n * n * n * n * (v - eK)</MathInline>
    </Alias>
    <Alias name="iNa">
      <MathInline>gbarNa * m * m * m * h * (v - eNa)</MathInline>
    </Alias>
    <Alias name="iLeak">
      <MathInline>gLeak * (v - eLeak)</MathInline>
    </Alias>
    <Alias name="u">
      <MathInline>v - v_rest</MathInline>
    </Alias>
    <Alias name="m_inf">
      <MathInline>m_alpha / (m_alpha + m_beta)</MathInline>
    </Alias>
    <Alias name="m_tau">
      <MathInline>1 / (m_alpha + m_beta)</MathInline>
    </Alias>
    <Alias name="h_inf">
      <MathInline>h_alpha / (h_alpha + h_beta)</MathInline>
    </Alias>
    <Alias name="h_tau">
      <MathInline>1 / (h_alpha + h_beta)</MathInline>
    </Alias>
    <Alias name="n_inf">
      <MathInline>n_alpha / (n_alpha + n_beta)</MathInline>
    </Alias>
    <Alias name="n_tau">
      <MathInline>1 / (n_alpha + n_beta)</MathInline>
    </Alias>      
    <Alias name="m_alpha">
      <MathInline>m_alpha_A * (m_alpha_V0 - u) / (exp((m_alpha_V0 - u) / m_alpha_K) - 1)</MathInline>
    </Alias>
    <Alias name="m_beta">
      <MathInline>m_beta_A * (u - m_beta_V0) / (exp((u - m_beta_V0) / m_beta_K) - 1)</MathInline>
    </Alias>
    <Alias name="n_alpha">
      <MathInline>n_alpha_A * (n_alpha_V0 - u) / (exp((n_alpha_V0 - u) / n_alpha_K) - 1)</MathInline>
    </Alias>
    <Alias name="n_beta">
      <MathInline>n_beta_A * exp((n_beta_V0 - u) / n_beta_K)</MathInline>
    </Alias>    
    <Alias name="h_alpha">
      <MathInline>h_alpha_A * exp((h_alpha_V0 - u) / h_alpha_K)</MathInline>
    </Alias>
    <Alias name="h_beta">
      <MathInline>h_beta_A / (1 + exp ((h_beta_V0 - u) / h_beta_K))</MathInline>
    </Alias>
  </Dynamics>
  <DynamicsProperties name="HodgkinHuxleyProperties">
    <Definition name="HodgkinHuxley"/>
    <Property name="m_alpha_A" units="per_ms_mV">
      <SingleValue>0.32</SingleValue>
    </Property>
    <Property name="m_alpha_V0" units="mV">
      <SingleValue>13</SingleValue>
    </Property>
    <Property name="m_alpha_K" units="mV">
      <SingleValue>4</SingleValue>
    </Property>
    <Property name="m_beta_A" units="per_ms_mV">
      <SingleValue>0.28</SingleValue>
    </Property>
    <Property name="m_beta_V0" units="mV">
      <SingleValue>40</SingleValue>
    </Property>
    <Property name="m_beta_K" units="mV">
      <SingleValue>5</SingleValue>
    </Property>
    <Property name="h_alpha_A" units="per_ms">
      <SingleValue>0.128</SingleValue>
    </Property>
    <Property name="h_alpha_V0" units="mV">
      <SingleValue>17</SingleValue>
    </Property>
    <Property name="h_alpha_K" units="mV">
      <SingleValue>18</SingleValue>
    </Property>
    <Property name="h_beta_A" units="per_ms">
      <SingleValue>4</SingleValue>
    </Property>
    <Property name="h_beta_V0" units="mV">
      <SingleValue>40</SingleValue>
    </Property>
    <Property name="h_beta_K" units="mV">
      <SingleValue>5</SingleValue>
    </Property>
    <Property name="n_alpha_A" units="per_ms_mV">
      <SingleValue>0.032</SingleValue>
    </Property>
    <Property name="n_alpha_V0" units="mV">
      <SingleValue>15</SingleValue>
    </Property>
    <Property name="n_alpha_K" units="mV">
      <SingleValue>5</SingleValue>
    </Property>
    <Property name="n_beta_A" units="per_ms">
      <SingleValue>0.5</SingleValue>
    </Property>
    <Property name="n_beta_V0" units="mV">
      <SingleValue>10</SingleValue>
    </Property>
    <Property name="n_beta_K" units="mV">
      <SingleValue>40</SingleValue>
    </Property>
    <Property name="gbarNa" units="nS">
      <SingleValue>20000</SingleValue>
    </Property>
    <Property name="gbarK" units="nS">
      <SingleValue>6000</SingleValue>
    </Property>
    <Property name="gLeak" units="nS">
      <SingleValue>10</SingleValue>
    </Property>
    <Property name="eLeak" units="mV">
      <SingleValue>-60</SingleValue>
    </Property>
    <Property name="eK" units="mV">
      <SingleValue>-90</SingleValue>
    </Property>
    <Property name="eNa" units="mV">
      <SingleValue>50</SingleValue>
    </Property>
    <Property name="C" units="pF">
      <SingleValue>200</SingleValue>
    </Property>
    <Property name="v_rest" units="mV">
      <SingleValue>-63.0</SingleValue>
    </Property>
    <Property name="v_threshold" units="mV">
      <SingleValue>-20</SingleValue>
    </Property>
  </DynamicsProperties>
  <Dimension name="per_time" t="-1"/>
  <Dimension name="voltage" m="1" l="2" t="-3" i="-1"/>
  <Dimension name="per_time_voltage" m="-1" l="-2" t="2" i="1"/>
  <Dimension name="conductance" m="-1" l="-2" t="3" i="2"/>
  <Dimension name="capacitance" m="-1" l="-2" t="4" i="2"/>
  <Dimension name="current" i="1"/>
  <Dimension name="currentDensity" i="1" l="-2"/>
  <Dimension name="temperature" k="1"/>
  <Dimension name="dimensionless"/>
  <Unit symbol="per_ms" dimension="per_time" power="3"/>
  <Unit symbol="mV" dimension="voltage" power="-3"/>
  <Unit symbol="per_ms_mV" dimension="per_time_voltage" power="6"/>
  <Unit symbol="nS" dimension="conductance" power="-9"/>
  <Unit symbol="pF" dimension="capacitance" power="-12"/>
  <Unit symbol="nA" dimension="current" power="-3"/>
  <Unit symbol="unitless" dimension="dimensionless" power="0"/>
  <Unit symbol="ms" dimension="time" power="-3"/>
  <Unit symbol="S" dimension="conductance" power="0"/>
  <Unit symbol="K" dimension="temperature" power="0"/>
  <Dimension name="time" t="1"/>
</NineML>
