<?xml version="1.0" encoding="UTF-8"?>
<NineML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://nineml.net/9ML/1.0"
  xsi:schemaLocation="http://nineml.incf.org/9ML/NineML_v0.3.xsd">
  <Dimension name="per_time" t="-1"/>
  <Dimension name="voltage" m="1" l="2" t="-3" i="-1"/>  
  <Dimension name="temperature" k="1"/>
  <Dimension name="conductanceDensity" m="-1" l="-4" t="3" i="2"/>
  <Dimension name="currentDensity" i="1" l="-2"/>
  <Dimension name="dimensionless"/>
  <Unit symbol="celsius" dimension="temperature" power="0" offset="273.15"/>
  <Unit symbol="K" dimension="temperature" power="0"/>
  <Dynamics name="Kv_A">
    <Parameter name="Aalpha_a" dimension="per_time"/>
    <Parameter name="Kalpha_a" dimension="voltage"/>
    <Parameter name="V0alpha_a" dimension="voltage"/>
    <Parameter name="Abeta_a" dimension="per_time"/>
    <Parameter name="Kbeta_a" dimension="voltage"/>
    <Parameter name="V0beta_a" dimension="voltage"/>
    <Parameter name="Aalpha_b" dimension="per_time"/>
    <Parameter name="Kalpha_b" dimension="voltage"/>
    <Parameter name="V0alpha_b" dimension="voltage"/>
    <Parameter name="Abeta_b" dimension="per_time"/>
    <Parameter name="Kbeta_b" dimension="voltage"/>
    <Parameter name="V0beta_b" dimension="voltage"/>
    <Parameter name="V0_ainf" dimension="voltage"/>
    <Parameter name="K_ainf" dimension="voltage"/>
    <Parameter name="V0_binf" dimension="voltage"/>
    <Parameter name="K_binf" dimension="voltage"/>
    <Parameter name="gbar" dimension="conductanceDensity"/>
    <AnalogReceivePort name="temperature" dimension="temperature"/>
    <AnalogReceivePort name="v" dimension="voltage"/>
    <AnalogSendPort name="g" dimension="conductanceDensity"/>
    <StateVariable name="a" dimension="dimensionless">
      <Initial>a_inf</Initial>
    </StateVariable>
    <StateVariable name="b" dimension="dimensionless">
      <Initial>b_inf</Initial>
    </StateVariable>
    <Alias name="g">
      <MathInline>gbar * a * a * a * b</MathInline>
    </Alias>
    <Regime name="default">
      <TimeDerivative variable="a">
        <MathInline>(a_inf - a) / a_tau</MathInline>
      </TimeDerivative>
      <TimeDerivative variable="b">
        <MathInline>(b_inf - b) / b_tau</MathInline>
      </TimeDerivative>
    </Regime>
    <Alias name="Q10">
      <MathInline>pow(3, ((temperature - reference_temperature) / ten_degrees))</MathInline>
    </Alias>
    <Alias name="a_inf">
      <MathInline>1 / (1 + exp((v - V0_ainf) / K_ainf))</MathInline>
    </Alias>
    <Alias name="a_tau">
      <MathInline>1 / (alpha_a + beta_a)</MathInline>
    </Alias>
    <Alias name="b_inf">
      <MathInline>1 / (1 + exp((v - V0_binf) / K_binf))</MathInline>
    </Alias>
    <Alias name="b_tau">
      <MathInline>1 / (alpha_b + beta_b)</MathInline>
    </Alias>
    <Alias name="alpha_a">
      <MathInline>Q10 * Aalpha_a / (exp((v - V0alpha_a)/Kalpha_a) + 1)</MathInline>
    </Alias>
    <Alias name="beta_a">
      <MathInline>Q10 * (Abeta_a / exp((v - V0beta_a) /
        Kbeta_a))</MathInline>
    </Alias>
    <Alias name="alpha_b">
      <MathInline>Q10 * Aalpha_b /(exp((v - V0alpha_b)/Kalpha_b) + 1)</MathInline>
    </Alias>
    <Alias name="beta_b">
      <MathInline>Q10 * Abeta_b /(exp((v - V0beta_b)/Kbeta_b) + 1)</MathInline>
    </Alias>
    <Constant name="reference_temperature" value="25.5" units="celsius"/>
    <Constant name="ten_degrees" value="10" units="K"/>
  </Dynamics>
</NineML>
